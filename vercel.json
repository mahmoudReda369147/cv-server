
{
  "version": 2,
  "functions": {
    "src/server.js": {
      "runtime": "vercel-node@20.0.0",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "src/server.js" },
    { "src": "/(.*)", "dest": "src/server.js" }
  ],
  "installCommand": "npm ci && npx prisma generate"
}
